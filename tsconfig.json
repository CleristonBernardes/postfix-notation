{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "baseUrl": "src/",
      "allowJs": true,
      "paths": {
        "*": [
          "node_modules/*",
          "src/types/*"
        ]
      }
    },
    "include": [
      "src/**/*"
    ]
  }
  